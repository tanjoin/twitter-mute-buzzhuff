{
  "manifest_version": 2,
  "name": "twitter-mute-buzzfuff",
  "version": "0.2.2",
  "description": "鬱陶しいメディアを目に入らなくする",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [ "https://twitter.com/*" ],
      "js": [ "main.js" ]
    }
  ],
  "options_page": "options.html"
}